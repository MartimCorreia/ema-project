<div class="dash-top">
  <div class="container medical-trauma-box d-flex flex-column justify-content-center">
    <div class="img-outer-box mb-3">
      <%= image_tag "data:image/jpeg;base64,", class:"medical-img" %>
      <h2 class="medical-h2 text-center"><%= link_to "Medical", treatments_path(age_group: params[:age_group], location: params[:location], type: "medical") %></h2>
    </div>

    <div class="img-outer-box">
      <%= image_tag "https://i0.wp.com/images-prod.healthline.com/hlcmsresource/images/News/tech/1296x728_broken_bones.jpg?w=1155&h=1528", class:"trauma" %>
      <h2 class="trauma-h2 text-center"><%= link_to "Trauma", treatments_path(age_group: params[:age_group], location: params[:location], type: "trauma") %></h2>
    </div>
  </div>
</div>
